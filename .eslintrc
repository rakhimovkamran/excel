{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true,
        "jest": true
    },

    "rules": {
        "quotes": "off",
        "object-curly-spacing": "off",
        "indent": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off",
        "space-before-function-paren": "off",
        "max-len": "off"
    },

    "extends": ["eslint:recommended", "google"],
    "plugins": ["jest"]

}
